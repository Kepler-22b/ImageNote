{% block form_row %}
    <div class="form_row">
        <div class="">
            {{ form_label(form) }}
        </div>
        <div>
            {{ form_errors(form) }}
        </div>
        <div>
            {{ form_widget(form, {attr: {class: 'area'}}) }}
        </div>
    </div>
{% endblock form_row %}